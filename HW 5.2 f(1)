f = @(x) (x^3)/sqrt(x^4-1);
Ctrap(f,2,3,16)

ans =

    2.5357

Ctrap(f,2,3,32)

ans =

    2.5357

int(f(x),2,3)
 
ans =
 
2*5^(1/2) - 15^(1/2)/2
 
format long
Ctrap(f,2,3,16)

ans =

   2.535672258240919

Ctrap(f,2,3,32)

ans =

   2.535651278545044

%error for both results
2.535672258240919 - (2*5^(1/2) - 15^(1/2)/2)

ans =

     2.797634504769775e-05

%Error m= 16
 2.535672258240919 - (2*5^(1/2) - 15^(1/2)/2)

ans =

     2.797634504769775e-05

clc
diary HW
